        <argument
            android:name="fooditem"
            app:argType="Model.FoodItems" />
        <action
            android:id="@+id/action_selectedItemEditAdmin_to_editItems"
            app:destination="@id/editItems"
            app:enterAnim="@anim/fade_in_center"
            app:exitAnim="@anim/fade_out_center"
