            app:destination="@id/category"
            app:enterAnim="@anim/fade_in_center"
            app:exitAnim="@anim/fade_out_center"
            app:popEnterAnim="@anim/fade_in_center"
            app:popExitAnim="@anim/fade_out_center" />
